{
  "name": "web",
  "version": "1.0.0",
  "engines": {
    "node": ">=12.x"
  },
  "private": "true",
  "description": "Web application for MeetingBook",
  "keywords": [
    "web",
    "application",
    "html",
    "css",
    "js"
  ],
  "author": "Amiko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:meetingbook/meetingbook.git"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --quiet --fix .",
    "format": "prettier --write . --ignore-path .gitignore",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "PORT=3000 serve app",
    "serve": "PORT=3000 serve app"
  },
  "dependencies": {
    "serve": "12.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babel-jest": "^27.0.6",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-prettier": "^3.4.0",
    "ghooks": "^2.0.4",
    "jest": "^27.0.6",
    "prettier": "^2.3.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "cd web && npm run format && npm run lint:fix"
    }
  }
}
